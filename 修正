def qqq(d):
    if len(d)==1:
        return 1
    if len(d)==2:
        return 3
    if len(d)==3:
        return 4
for p in range(0,qqq(d)):
    if d[p]=='十':
        d=d.replace(d[p],'10')
    if d[p]=='九':
        d=d.replace(d[p],'9')
    if d[p]=='八':
        d=d.replace(d[p],'8')
